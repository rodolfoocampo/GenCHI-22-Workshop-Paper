@article{stanley04,
	abstract = {Two ma jor goals in machine learning are the discovery and improvement of solutions to complex problems. In this paper, we argue that complexification, i.e. the incremental elaboration of solutions through adding new structure, achieves both these goals. We demonstrate the power of complexification through the NeuroEvolution of Augmenting Topologies (NEAT) method, which evolves increasingly complex neural network architectures. NEAT is applied to an open-ended coevolutionary robot duel domain where robot controllers compete head to head. Because the robot duel domain supports a wide range of strategies, and because coevolution benefits from an escalating arms race, it serves as a suitable testbed for studying complexification. When compared to the evolution of networks with fixed struc- 
ture, complexifying evolution discovers significantly more sophisticated strategies. The results suggest that in order to discover and improve complex solutions, evolution, and search in general, should be allowed to complexify as well as optimize. },
	annote = {Complexification facilitates discovery and improvement of solutions.

This process is based on three technical components: (1) Keeping track of which genes match up with which among differently sized genomes throughout evolution; (2) speciating the population so that solutions of differing complexity can exist independently; and (3) starting evolution with a uniform population of small networks.

Now consider 'open-ended problems' (coevolution).

The main results were that (1) evolution did complexify when possible, (2) complexification led to elaboration, and (3) significantly more sophisticated and successful strategies were evolved with complexification than without it.},
	author = {Kenneth O. Stanley and Risto Miikkulainen},
	date-added = {2013-02-07 09:16:27 +0000},
	date-modified = {2013-02-07 09:16:28 +0000},
	journal = {Journal of Artificial Intelligence Research},
	keywords = {artificial life, evolution},
	month = {February},
	pages = {63--100},
	title = {Competitive Coevolution through Evolutionary Complexification},
	volume = {21},
	year = {2004}}

