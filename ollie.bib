	date-added = {2013-02-07 09:16:27 +0000},
	date-modified = {2013-02-07 09:16:27 +0000},
	howpublished = {For the exhibition Emo{\c c}{\~a}o Art.ficial 4.0 Emerg{\^e}ncia at Ita{\'u} Cultural, Sao Paolo, Brazil},
	keywords = {artificial life art},
